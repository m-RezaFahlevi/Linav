\begin_inset Formula 
\[
\langle\lambda\vec{x},\mu\vec{y}\rangle=\sum_{i=1}^{n}(\lambda x_{i})(\mu y_{i})=\sum_{i=1}^{n}(\lambda\mu)(x_{i}y_{i})=\lambda\mu\sum_{i=1}^{n}x_{i}y_{i}=\lambda\mu\langle\vec{x},\vec{y}\rangle
\]

\end_inset


\end_layout

